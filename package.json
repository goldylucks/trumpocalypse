{
  "name": "trumpocalypse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --open --hot --inline --progress --colors --port 3000",
    "clean": "rm -rf client-dist",
    "prebuild": "npm run clean",
    "prebuild:staging": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors",
    "build:staging": "NODE_ENV=staging webpack --progress --colors",
    "server": "nodemon server/server.js --ignore client --ignore client-dist --ignore node_modules",
    "server:prod": "NODE_ENV=production node server/server.js",
    "lint": "eslint client server --ignore-path .gitignore"
  },
